<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MapsActivity"></activity>
        <activity android:name=".RiderMainActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".CartMainActivity" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".DeliverySignUpActivity" />
        <activity android:name=".UserSignUpActivity" />
        <activity android:name=".DeliverSignInActivity" />
        <activity android:name=".UserSignInActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>